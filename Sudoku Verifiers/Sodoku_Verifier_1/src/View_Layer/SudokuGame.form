<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="View_Layer.SudokuGame">
  <grid id="27dc6" binding="mainPanel" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="600" height="700"/>
    </constraints>
    <properties>
      <minimumSize width="550" height="650"/>
      <preferredSize width="580" height="680"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="b8e5f" binding="topPanel" layout-manager="BorderLayout" hgap="0" vgap="10">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="none">
          <size top="10" left="10" bottom="10" right="10"/>
        </border>
        <children>
          <component id="c9f6a" class="javax.swing.JLabel" binding="titleLabel">
            <constraints border-constraint="North"/>
            <properties>
              <font size="24" style="1"/>
              <horizontalAlignment value="0"/>
              <text value="Sudoku Game"/>
            </properties>
          </component>
          <grid id="d7a3b" binding="infoPanel" layout-manager="GridLayoutManager" row-count="1" column-count="2" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
            <margin top="5" left="0" bottom="5" right="0"/>
            <constraints border-constraint="Center"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="e8b4c" class="javax.swing.JLabel" binding="statusLabel">
                <constraints>
                  <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14"/>
                  <text value="Status: Playing"/>
                </properties>
              </component>
              <component id="f9c5d" class="javax.swing.JLabel" binding="emptyCellsLabel">
                <constraints>
                  <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false"/>
                </constraints>
                <properties>
                  <font size="14"/>
                  <text value="Empty cells: 0"/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="g7d2e" binding="boardPanel" layout-manager="GridLayoutManager" row-count="1" column-count="1" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="10" left="10" bottom="10" right="10"/>
        <constraints border-constraint="Center"/>
        <properties>
          <background color="-1"/>
        </properties>
        <border type="none"/>
        <children/>
      </grid>
      <grid id="h8e3f" binding="controlPanel" layout-manager="GridLayoutManager" row-count="1" column-count="4" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
        <margin top="15" left="20" bottom="15" right="20"/>
        <constraints border-constraint="South"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="i9f4a" class="javax.swing.JButton" binding="verifyButton">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="120" height="35"/>
              </grid>
            </constraints>
            <properties>
              <font size="14"/>
              <text value="Verify"/>
            </properties>
          </component>
          <component id="j7a5b" class="javax.swing.JButton" binding="solveButton">
            <constraints>
              <grid row="0" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="120" height="35"/>
              </grid>
            </constraints>
            <properties>
              <enabled value="false"/>
              <font size="14"/>
              <text value="Solve"/>
            </properties>
          </component>
          <component id="k8b6c" class="javax.swing.JButton" binding="undoButton">
            <constraints>
              <grid row="0" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="120" height="35"/>
              </grid>
            </constraints>
            <properties>
              <font size="14"/>
              <text value="Undo"/>
            </properties>
          </component>
          <component id="l9c7d" class="javax.swing.JButton" binding="newGameButton">
            <constraints>
              <grid row="0" column="3" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false">
                <preferred-size width="120" height="35"/>
              </grid>
            </constraints>
            <properties>
              <font size="14"/>
              <text value="Exit"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
